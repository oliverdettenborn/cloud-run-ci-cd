{
  "name": "cloud-run-ci-cd",
  "version": "1.0.0",
  "description": "Template to create new typescript express applications",
  "main": "dist/server.js",
  "scripts": {
    "prepare": "husky install",
    "dev": "NODE_ENV=development nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' -e ts",
    "start": "NODE_ENV=production node ./dist/server.js",
    "build": "rm -rf ./dist && ttsc -p tsconfig-build.json",
    "test": "jest --passWithNoTests --noStackTrace --runInBand --collectCoverage",
    "test:watch": "npm run test -- --watch",
    "lint:staged": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oliverdettenborn/cloud-run-ci-cd.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oliverdettenborn/cloud-run-ci-cd/issues"
  },
  "homepage": "https://github.com/oliverdettenborn/cloud-run-ci-cd#readme",
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.3.1",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^11.2.6",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.3",
    "typescript-transform-paths": "^2.2.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.5.0"
  }
}
